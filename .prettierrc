{
    "$schema": "https://json.schemastore.org/prettierrc",
    "singleQuote": true,
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "endOfLine": "auto",
    "bracketSpacing": false,
    "arrowParens": "always",
    "bracketSameLine": false,
    "printWidth": 80,
    "trailingComma": "all"
}
